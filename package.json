{
  "name": "aws-sigv4-fetch",
  "version": "2.1.2-next.0",
  "description": "SignatureV4 fetch implemented with official @aws-sdk v3",
  "packageManager": "pnpm@8.8.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "README.md"
  ],
  "scripts": {
    "test": "vitest",
    "pretest": "pnpm build && pnpm start",
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "pnpm build",
    "lint": "pnpm biome lint ./src",
    "format": "pnpm biome format ./src --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zirkelc/aws-sigv4-fetch.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zirkelc/aws-sigv4-fetch/issues"
  },
  "homepage": "https://github.com/zirkelc/aws-sigv4-fetch#readme",
  "dependencies": {
    "@aws-crypto/sha256-js": "^4.0.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.359.0",
    "@aws-sdk/credential-provider-node": "^3.358.0",
    "@aws-sdk/protocol-http": "^3.357.0",
    "@aws-sdk/querystring-parser": "^3.357.0",
    "@aws-sdk/signature-v4": "^3.357.0",
    "@aws-sdk/types": "^3.357.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.1",
    "@tsconfig/node16": "^16.1.0",
    "cross-fetch": "^4.0.0",
    "typescript": "^5.2.0",
    "vitest": "^0.34.6"
  }
}